{
  "name": "angular-testing-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:otherport": "ng serve --port 4300",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build",
    "build:prod": "ng build --aot --configuration=production --progress=true --outputHashing=bundles --baseHref=/web/",
    "test": "ng test --code-coverage",
    "clean": "npm cache clean --force",
    "webdriver:clean": "webdriver-manager clean",
    "webdriver:update": "webdriver-manager update --chrome --ie --gecko",
    "webdriver:start": "webdriver-manager start --detach",
    "webdriver:stop": "webdriver-manager shutdown",
    "e2e:local": "ng e2e --configuration=local --webdriver-update=true",
    "e2e:fastlocal": "protractor e2e/protractor.conf.js --baseUrl=http://localhost:4200",
    "e2e:prod": "protractor e2e/protractor.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "ag-grid-angular": "^20.2.0",
    "ag-grid-community": "^20.2.0",
    "@angular/router": "~7.2.0",
    "bootstrap": "^4.3.1",
    "@ng-bootstrap/ng-bootstrap": "^4.1.1",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@types/chai-as-promised": "^7.1.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-json": "^1.0.0",
    "chai-smoothie": "^0.3.2",
    "cucumber": "^3.0.6",
    "cucumber-html-report": "^0.6.5",
    "cucumber-html-reporter": "^4.0.4",
    "@types/jasmine": "~2.8.8",
    "@types/cucumber": "^4.0.4",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "karma-htmlfile-reporter": "~0.3.8",
    "karma-jasmine": "~2.0.1",
    "karma-sonarqube-unit-reporter": "~0.0.18",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "protractor-cucumber-framework": "^6.1.1",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "sonar-scanner": "~3.1.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
